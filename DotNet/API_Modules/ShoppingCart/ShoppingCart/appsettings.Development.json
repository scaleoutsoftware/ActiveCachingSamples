{
  "Serilog": {

    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning",
        "Scaleout.Client": "Warning"
      }
    },

    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }
}
